{
  "files.exclude": {
    ".vscode": true,
    "**/*.o": true,
    "**/*.d": true,
    "**/*.bc": true,
    "**/*.i": true,
    "**/*.s": true,
    "**/*.a": true,
  },
  "files.associations": {
    "ft.h": "c",
    "stddef.h": "c"
  },
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/src"
  ],
  "c-cpp-flylint.includePaths": [
    "${workspaceFolder}/src"
  ],
}
